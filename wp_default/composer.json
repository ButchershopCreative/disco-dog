{
  "repositories":[
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "jarednova/timber": ">=0.18.1",
    "wpackagist-plugin/pods": ">=2.6.5.2",
    "wpackagist-plugin/taxonomy-terms-order": ">=1.4.0",
    "wpackagist-plugin/post-types-order": ">=1.7.4",
    "wpackagist-plugin/wordpress-importer": ">=0.6.1",
    "wpackagist-plugin/google-sitemap-generator": ">=4.0.8",
    "wpackagist-plugin/regenerate-thumbnails": ">=2.2.4"
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
